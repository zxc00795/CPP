#include <stdio.h>
#include <stdlib.h>
int main()
{
    printf("Hello, World! \n");
    return 0;
}
#######################################
-------------我是分隔線-----------------
#######################################
#include <iostream>
using namespace std;
int main()
{
    cout << "Hello, world!" << endl;
    return 0;
}
#######################################
-------------我是分隔線-----------------
#######################################
public class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Hello World");
    }
}
#######################################
-------------我是分隔線-----------------
#######################################
#include<stdio.h>
 
int main()
{
    int i,j,k;
    printf("\n");
    for(i=1;i<5;i++) { 
        for(j=1;j<5;j++) {
            for (k=1;k<5;k++) { 
                if (i!=k&&i!=j&&j!=k) { 
                    printf("%d,%d,%d\n",i,j,k);
                }
            }
        }
    }
}
#######################################
-------------我是分隔線-----------------
#######################################
